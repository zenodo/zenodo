<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://datacite.org/schema/kernel-4" targetNamespace="http://datacite.org/schema/kernel-4" elementFormDefault="qualified">
  <xs:simpleType name="dateType" id="dateType">
    <xs:annotation>
      <xs:documentation>The type of date. Use RKMSâ€ISO8601 standard for depicting date ranges.To indicate the end of an embargo period, use Available. To indicate the start of an embargo period, use Submitted or Accepted, as appropriate.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="Accepted"/>
      <xs:enumeration value="Available"/>
      <xs:enumeration value="Collected"/>
      <xs:enumeration value="Copyrighted"/>
      <xs:enumeration value="Created"/>
      <xs:enumeration value="Issued"/>
      <xs:enumeration value="Other"/>
      <xs:enumeration value="Submitted"/>
      <xs:enumeration value="Updated"/>
      <xs:enumeration value="Valid"/>
    </xs:restriction>
  </xs:simpleType>
</xs:schema>
