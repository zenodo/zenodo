{
  "template": "__SEARCH_INDEX_PREFIX__stats-record-view-*",
  "settings": {
    "index.mapper.dynamic": false,
    "index": {
      "refresh_interval": "5s"
    }
  },
  "mappings": {
    "record-view-day-aggregation": {
      "_all": {
        "enabled": false
      },
      "date_detection": false,
      "numeric_detection": false,
      "dynamic": false,
      "properties": {
        "timestamp": {
          "type": "date",
          "format": "strict_date_hour_minute_second"
        },
        "count": {
          "type": "integer",
          "index": "not_analyzed"
        },
        "unique_count": {
          "type": "integer",
          "index": "not_analyzed"
        },
        "record_id": {
          "type": "string",
          "index": "not_analyzed"
        },
        "recid": {
          "type": "string",
          "index": "not_analyzed"
        },
        "conceptrecid": {
          "type": "string",
          "index": "not_analyzed"
        },
        "doi": {
          "type": "string",
          "index": "not_analyzed"
        },
        "conceptdoi": {
          "type": "string",
          "index": "not_analyzed"
        },
        "communities": {
          "type": "string",
          "index": "not_analyzed"
        },
        "owners": {
          "type": "integer",
          "index": "not_analyzed"
        },
        "is_parent": {
          "type": "boolean",
          "index": "not_analyzed"
        }
      }
    },
    "record-view-agg-bookmark": {
      "date_detection": false,
      "properties": {
        "date": {
          "type": "date",
          "format": "date_optional_time"
        }
      }
    },
    "record-view-all-versions-agg-bookmark": {
      "date_detection": false,
      "properties": {
        "date": {
          "type": "date",
          "format": "date_optional_time"
        }
      }
    }
  },
  "aliases": {
    "__SEARCH_INDEX_PREFIX__stats-record-view": {}
  }
}
